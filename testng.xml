<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Learning TestNG" parallel="tests">
	<listeners>
		<listener class-name="TestNG_Learning.CustomListeners" />
		<listener class-name="ExtentListeners.ExtentListener1" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>

	<test thread-count="5" name="Test_Annotations">
		<groups>
			<run>
				<exclude name="high" />
			</run>
		</groups>
		<classes>
			<class name="TestNG_Learning.TestCase1_Annotations" />
		</classes>
	</test>

	<test thread-count="5" name="Test_Assertions">
		<classes>
			<class name="TestNG_Learning.TestCase2_Assert" />
		</classes>
	</test>

	<test thread-count="5" name="Test_Dependency">
		<classes>
			<class name="TestNG_Learning.TestCase3_DependsOn" />
		</classes>
	</test>
	<test thread-count="5" name="Test">
		<classes>
			<class name="TestNG_Learning.TestParameterization" />
		</classes>
	</test> <!-- Test -->
</suite> 